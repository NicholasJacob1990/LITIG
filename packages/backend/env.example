# ===== CONFIGURAÇÕES UNIPILE SDK =====

# Token de autenticação do Unipile (obrigatório)
UNIPILE_API_TOKEN=your_unipile_token_here
# ou alternativo
UNIFIED_API_KEY=your_unified_api_key_here

# Configuração do serviço preferido
# Opções: sdk_official, wrapper_nodejs, auto_fallback
UNIPILE_PREFERRED_SERVICE=sdk_official

# Região do servidor Unipile
# Opções: north-america, europe, australia
UNIPILE_SERVER_REGION=north-america

# DSN legacy (para compatibilidade com wrapper Node.js)
UNIPILE_DSN=api.unipile.com

# Configurações avançadas
UNIPILE_ENABLE_FALLBACK=true
UNIPILE_HEALTH_CHECK_INTERVAL=300
UNIPILE_TIMEOUT_SECONDS=30

# Rate limiting
UNIPILE_RATE_LIMIT_REQUESTS=100
UNIPILE_RATE_LIMIT_WINDOW=3600

# Logging
UNIPILE_LOG_LEVEL=INFO
UNIPILE_LOG_REQUESTS=false

# ===== OUTRAS CONFIGURAÇÕES =====
# (adicione outras variáveis conforme necessário) 
 

